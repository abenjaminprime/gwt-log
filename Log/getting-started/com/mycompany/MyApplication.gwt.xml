<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/tags/1.5.0/distro-source/core/src/gwt-module.dtd">
<module>

  <!-- Inherit the core Web Toolkit stuff.-->
  <inherits name="com.google.gwt.user.User" />

  <!-- Inherit logging support                   -->
  <inherits name="com.allen_sauer.gwt.log.gwt-log" />

  <!-- Change default value from 'OFF' to 'DEBUG' -->
  <extend-property name="log_level" values="DEBUG" />
  <set-property name="log_level" value="DEBUG" />

  <!-- Specify the app entry point class.               -->
  <entry-point class="com.mycompany.client.MyApplication" />

</module>
